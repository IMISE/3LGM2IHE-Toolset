    rdfs:label "Pull Notification" ;
    rdfs:subPropertyOf :iti .

:iti-78
    ontology:abbreviation "ITI-78" ;
    terms:description "Performs a query against a patient demographics supplier using HTTP, REST, and JSON/XML message encoding." ;
