    a owl:ObjectProperty ;
    rdfs:label "Query Post - Processing Worklist"^^xsd:string ;
    rdfs:subPropertyOf :rad .

:rad-38
    ontology:abbreviation "RAD-38"^^xsd:string ;
    terms:description "A worklist client (Image Creator) notifies the worklist provider (Post-Processing Manager) that it has claimed the workitem (i.e. started a General Purpose Scheduled Procedure Step)."^^xsd:string ;
    a owl:ObjectProperty ;
    rdfs:label "Workitem Claimed"^^xsd:string ;
    rdfs:subPropertyOf :rad .

:rad-39
    ontology:abbreviation "RAD-39"^^xsd:string ;
    terms:description "A worklist client (Image Creator) notifies the worklist provider (Post-Processing Manager) that it has started work (i.e. created a General Purpose Performed Procedure Step)."^^xsd:string ;
    a owl:ObjectProperty ;
    rdfs:label "Workitem Performed Procedure Step In Process"^^xsd:string ;
    rdfs:subPropertyOf :rad .

