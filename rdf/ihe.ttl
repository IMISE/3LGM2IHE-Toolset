    rdfs:label "Procedure Scheduled"^^xsd:string ;
    rdfs:subPropertyOf :rad .

:rad-40
    ontology:abbreviation "RAD-40"^^xsd:string ;
    terms:description "A worklist client (Image Creator) notifies the worklist provider (Post-Processing Manager) of the completion of a General Purpose Performed Procedure Step."^^xsd:string ;
    a owl:ObjectProperty ;
    rdfs:label "Workitem Performed Procedure Step Completed"^^xsd:string ;
    rdfs:subPropertyOf :rad .

:rad-41
    ontology:abbreviation "RAD-41"^^xsd:string ;
    terms:description "A worklist client (Image Creator) notifies the worklist provider (Post-Processing Manager) that it has finished the workitem (i.e. completed a General Purpose Scheduled Procedure Step)."^^xsd:string ;
    a owl:ObjectProperty ;
    rdfs:label "Workitem Completed"^^xsd:string ;
    rdfs:subPropertyOf :rad .

:rad-42
    ontology:abbreviation "RAD-42"^^xsd:string ;
    terms:description "The worklist provider informs other interested actors of the on-going status and completion of performed work."^^xsd:string ;
    a owl:ObjectProperty ;
    rdfs:label "Performed Work Status Update"^^xsd:string ;
    rdfs:subPropertyOf :rad .

:rad-43
    ontology:abbreviation "RAD-43"^^xsd:string ;
    terms:description "An Acquisition Modality or Image Creator sends measured or derived diagnostic evidence in the form of a DICOM Structured Report to the Image Archive."^^xsd:string ;
    a owl:ObjectProperty ;
    rdfs:label "Evidence Document Stored"^^xsd:string ;
    rdfs:subPropertyOf :rad .

:rad-44
    ontology:abbreviation "RAD-44"^^xsd:string ;
    terms:description "An Image Display queries the Image Archive for a list of entries representing Evidence Documents."^^xsd:string ;
    a owl:ObjectProperty ;
    rdfs:label "Query Evidence Documents"^^xsd:string ;
    rdfs:subPropertyOf :rad .

