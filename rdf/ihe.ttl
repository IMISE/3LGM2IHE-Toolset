                                dlgm2ihe:integrationProfile_dataProperty "SeR"^^xsd:string ;
                                ontology:abbreviation "AUTH_DECISIONS_MGR"^^xsd:string ;
                                terms:description "Actor that can perform Access Control decisions, evaluating requests for authorization. The result of this evaluation is the creation of an Authorization Decision that certifies the decision made."^^xsd:string ;
                                rdfs:label "Authorization Decisions Manager (SeR)"^^xsd:string .


