                rdfs:subPropertyOf dlgm2ihe:iti ;
                ontology:abbreviation "ITI-10" ;
                terms:description "Allows a Patient Identity Consumer to be notified by the Patient Identity Cross-reference Manager Actor of changes to the identification of a patient in Patient Identification Domains the Consumer is interested in." ;
