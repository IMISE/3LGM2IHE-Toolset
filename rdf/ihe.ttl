    a owl:NamedIndividual, :Actor ;
    rdfs:label "Department System Scheduler/Order Filler (LTW)"^^xsd:string ;
    :iheIntegrationProfile :LTW ;
    :lab-2 :LTW_OP ;
    :lab-3 :LTW_ORT ;
    :lab-4 :LTW_AM .

