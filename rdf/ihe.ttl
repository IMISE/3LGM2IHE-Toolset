    rdfs:subPropertyOf :toDo .

:provides_mandatory
    a owl:ObjectProperty ;
    rdfs:subPropertyOf :provides_optional .

:provides_optional
    a owl:ObjectProperty ;
    rdfs:subPropertyOf :toDo .

:rad
    a owl:ObjectProperty ;
    rdfs:label "RAD Transactions" ;
    rdfs:subPropertyOf :iheTransaction .

:rad-1
