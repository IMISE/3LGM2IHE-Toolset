###  https://www.3lgm2.de/resource/ihe/DSG
dlgm2ihe:DSG rdf:type owl:NamedIndividual ,
                      dlgm2ihe:IntegrationProfile ;
             dlgm2ihe:iheDomain dlgm2ihe:ITI ;
             dlgm2ihe:status "Final Text" ;
             ontology:abbreviation "DSG" ;
             terms:description "The Document Digital Signature (DSG) Profile is a Document Content Profile that provides general purpose methods of digitally signing of documents for communication and persistence. This method can be used within a Document Sharing infrastructure (e.g., XDS, XCA, XDM, XDR, and MHD). There are three methods of digital signature provided: Enveloping, Detached, and SubmissionSet." ;
             rdfs:label "Document Digital Signature" ;
             rdfs:seeAlso "https://profiles.ihe.net/ITI/TF/Volume1/ch-37.html"^^<http://schema.org/url> .


###  https://www.3lgm2.de/resource/ihe/DSUB
dlgm2ihe:DSUB rdf:type owl:NamedIndividual ,
                       dlgm2ihe:IntegrationProfile ;
              dlgm2ihe:iheDomain dlgm2ihe:ITI ;
              dlgm2ihe:status "Final Text" ;
              ontology:abbreviation "DSUB" ;
              terms:description "This profile describes the use of subscription and notification mechanism for use within an XDS Affinity Domain and across communities. The subscription allows for the matching of metadata during the publication of a new document for a given patient, and results in the delivery of a notification." ;
              rdfs:label "Document Metadata Subscription" ;
              rdfs:seeAlso "https://profiles.ihe.net/ITI/TF/Volume1/ch-26.html"^^<http://schema.org/url> .


