    a owl:NamedIndividual, :Actor ;
    rdfs:label "Audit Consumer (ATNA)"^^xsd:string ;
    :iheIntegrationProfile :ATNA ;
    :iti-81 :ATNA_ARR ;
    :iti-82 :ATNA_ARR ;
    :mustBeGroupedWith :CT_TIME_CLIENT .

