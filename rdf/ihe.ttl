    a owl:ObjectProperty ;
    rdfs:label "Patient Registration"^^xsd:string ;
    rdfs:subPropertyOf :rad .

:rad-10
    ontology:abbreviation "RAD-10"^^xsd:string ;
    terms:description "An Acquisition Modality or Image Creator requests that the Image Manager take responsibility for the specified DICOM objects (images, GSPS objects, Key Image Notes, Evidence Documents or any combination thereof) that the requestor stored in the Image Archive."^^xsd:string ;
    a owl:ObjectProperty ;
    rdfs:label "Storage Commitment"^^xsd:string ;
    rdfs:subPropertyOf :rad .

:rad-11
    ontology:abbreviation "RAD-11"^^xsd:string ;
