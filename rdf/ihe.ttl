                  terms:description "A system unit that receives and collects audit records from multiple systems."^^xsd:string ;
                  rdfs:label "Audit Record Repository (ATNA)"^^xsd:string ;
                  rdfs:seeAlso "https://profiles.ihe.net/ITI/TF/Volume1/ch-9.html#9.1"^^<http://schema.org/url> .


