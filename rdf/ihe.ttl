    a owl:ObjectProperty ;
    rdfs:label "Images Availability Query"^^xsd:string ;
    rdfs:subPropertyOf :rad .

:rad-12
    ontology:abbreviation "RAD-12"^^xsd:string ;
    terms:description "The ADT Patient Registration System informs the Order Placer and the Department System Scheduler/Order Filler of new information for a particular patient. The Department System Scheduler may then further inform the Image Manager."^^xsd:string ;
