:rad-22
    ontology:abbreviation "RAD-22"^^xsd:string ;
    a owl:ObjectProperty ;
    rdfs:label "Intentionally unassigned"^^xsd:string ;
    rdfs:subPropertyOf :rad .

:rad-23
