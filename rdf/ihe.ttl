    rdfs:label "Print Request with Presentation LUT"^^xsd:string ;
    rdfs:subPropertyOf :rad .

:rad-24
    ontology:abbreviation "RAD-24"^^xsd:string ;
    terms:description "A Report Creator sends a draft or final report to the Report Manager."^^xsd:string ;
    a owl:ObjectProperty ;
    rdfs:label "Report Submission"^^xsd:string ;
    rdfs:subPropertyOf :rad .

:rad-25
    ontology:abbreviation "RAD-25"^^xsd:string ;
    terms:description "A Report Manager sends a draft or final Report to the Report Repository."^^xsd:string ;
    a owl:ObjectProperty ;
    rdfs:label "Report Issuing"^^xsd:string ;
    rdfs:subPropertyOf :rad .

:rad-26
    ontology:abbreviation "RAD-26"^^xsd:string ;
    terms:description "A Report Reader provides a set of criteria to select the list of entries representing reports by patient, study, series, or report known by the Report Repository or External Report Repository Access."^^xsd:string ;
    a owl:ObjectProperty ;
    rdfs:label "Query Reports"^^xsd:string ;
    rdfs:subPropertyOf :rad .

:rad-27
