    rdfs:label "Import Procedure Step in Progress"^^xsd:string ;
    rdfs:subPropertyOf :rad .

:rad-6
    ontology:abbreviation "RAD-6"^^xsd:string ;
    terms:description "An Acquisition Modality notifies the Performed Procedure Step Manager of the start of a new Procedure Step and the PPS Manager informs the Department System and Image Manager."^^xsd:string ;
